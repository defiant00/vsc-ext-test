{
  "name": "vsc-ext-test",
  "displayName": "vsc_ext_test",
  "description": "VSCode Extension Test",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onCommand:vsc-ext-test.helloWorld"
  ],
  "main": "./client/out/extension.js",
  "contributes": {
    "languages": [{
      "id": "vsc_ext_test",
      "aliases": ["vsc_ext_test"],
      "extensions": [".vscet"],
      "configuration": "./language-configuration.json"
    }],
    "commands": [
      {
        "command": "vsc-ext-test.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -b",
    "watch": "tsc -b -w",
    "lint": "eslint ./client/src ./server/src --ext ts"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/glob": "^8.0.0",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "typescript": "^4.9.3"
  }
}
